class Solution {
public:
    int longestConsecutive(vector<int>& nums) {
        set<int> s (nums.begin(), nums.end());
        int ans = 0;
        for (auto num : s) {
            int res = 1;
            while (s.count(++num)) {
                s.erase(num);
                res++;
            }
            ans = max(ans, res);
        }
        return ans;
    }
};
