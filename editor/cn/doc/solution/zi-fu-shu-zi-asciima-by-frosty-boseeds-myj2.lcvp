int countSeniors(char ** details, int detailsSize){
    int i,j=15;
    int Num=0;
    for(i=0;i<detailsSize;i++){
        int a=details[i][11]-'0';
        int b=details[i][12]-'0';
        if(a*10+b>60)
        Num++;
    }
    return Num;
}
