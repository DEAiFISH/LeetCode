class Solution {
    public boolean isValid(String s) {
        Stack<Character> stack = new Stack<>();
        for (char c : s.toCharArray()){
            if(c == '(' || c == '{' || c == '['){
                stack.push(c);
            }else if(!stack.isEmpty()){
                switch (c){
                    case ')':{
                        if (stack.peek() == '('){
                            stack.pop();
                        }else {
                            stack.push(c);
                        }
                        break;
                    }
                    case ']':{
                        if (stack.peek() == '['){
                            stack.pop();
                        }else {
                            stack.push(c);
                        }
                        break;
                    }
                    case '}':{
                        if (stack.peek() == '{'){
                            stack.pop();
                        }else {
                            stack.push(c);
                        }
                        break;
                    }
                }
            }else {
                return false;
            }
        }
        return stack.isEmpty();
    }
}
